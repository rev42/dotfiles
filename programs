sudo snap install spotify
sudo snap install vscode --classic
sudo apt-get update
sudo apt-get install -y openvpn network-manager-openvpn easy-rsa network-manager-openvpn-gnome network-manager-pptp network-manager-vpnc libnss3 git terminator vim vlc p7zip git gparted curl terminator filezilla htop zlib1g-dev nfs-kernel-server zsh
sudo add-apt-repository ppa:ondrej/php
sudo apt-get update
sudo apt-get install php7.2-cli
curl -sS https://getcomposer.org/installer | php
sudo mv composer.phar /usr/local/bin/composer
wget https://dl.google.com/linux/direct/google-chrome-stable_current_amd64.deb
wget https://dev.mysql.com/get/Downloads/MySQLGUITools/mysql-workbench-community_8.0.19-1ubuntu18.04_amd64.deb
dpkg -i google-chrome-stable_current_amd64.deb
dpkg -i mysql-workbench-community_8.0.19-1ubuntu18.04_amd64.deb

sudo curl -L https://github.com/docker/compose/releases/download/1.25.4/docker-compose-`uname -s`-`uname -m` -o /usr/local/bin/docker-compose
sudo chmod +x /usr/local/bin/docker-compose
